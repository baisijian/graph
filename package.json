{
  "name": "graphtest",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.js --progress --colors",
    "clearBuild": "IF EXIST build del \"build\\*.*\" /Q",
    "clearProd": "IF EXIST prod del \"prod\\*.*\" /Q",
    "build": "webpack --config webpack.config.js --progress --profile --colors",
    "prod": "npm run clearProd && webpack -p --config webpack.prod.config.js --progress --colors",
    "cloud": "npm run clearProd && webpack -p --config webpack.cloud.config.js --progress --colors",
    "eslint": "eslint app --ext .js",
    "eslintFix": "eslint app --ext .js --fix",
    "stylelint": "stylelint app/**/*.css",
    "mockapi": "json-server -p 8000 ./mock_api/jsonserver.config.js --delay 1000 --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.16.0",
    "react-d3": "^0.4.0",
    "react-hot-loader": "^1.3.0",
    "react-transform-hmr": "^1.0.4",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "@vx/axis": "0.0.178",
    "@vx/glyph": "0.0.170",
    "@vx/gradient": "0.0.165",
    "@vx/group": "0.0.170",
    "@vx/hierarchy": "0.0.170",
    "@vx/mock-data": "0.0.165",
    "@vx/scale": "0.0.178",
    "@vx/shape": "0.0.178",
    "d3-array": "^1.2.4",
    "d3-hierarchy": "^1.1.8",
    "d3-shape": "^1.2.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  }
}
